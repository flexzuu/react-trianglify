{
  "name": "react-trianglify",
  "version": "0.1.0",
  "description": "A react component that wraps trianglify to generate svg's",
  "main": "./dist/trianglify.js",
  "scripts": {
    "watch:test": "npm t -- -w --reporter min",
    "test": "mocha --reporter spec src/*.test.* --compilers js:babel-register",
    "prebuild": "rimraf dist/*",
    "build": "babel src --out-dir dist --ignore *.test.*"
  },
  "keywords": [
    "react",
    "component",
    "trianglify",
    "svg"
  ],
  "author": "Jonas Faber",
  "license": "MIT",
  "bugs": "https://github.com/flexzuu/react-trianglify/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flexzuu/react-trianglify.git"
  },
  "homepage": "https://github.com/flexzuu/react-trianglify",
  "dependencies": {
    "react": "^15.1.0",
    "recompose": "^0.20.0",
    "trianglify": "^0.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "enzyme": "^2.3.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "mocha": "^2.5.3",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "rimraf": "^2.5.2"
  }
}
